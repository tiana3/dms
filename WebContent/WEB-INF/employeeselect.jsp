                    <tbody>
                    <%HttpSession s = request.getSession();
                    String name = s.getAttribute("employeename").toString();
                    ArrayList<Employee> employees = EmployeeDaoImlp.getEmployee(name);
                    for(Employee e:employees){%>
                    <tr class="info" id="1">
                        <td>
                            <%=e.getEmployeeId()%>
                        </td>
                        <td>
                            <%=e.getEmployeeName()%>
                        </td>
                        <td>
                            <%=e.getGender()%>
                        </td>
                        <td>
                        <%=e.getPhone()%>
                        </td>
                        <td>
                        <%=e.getIdCard()%>
                        </td>
                        <td>
                        <%=e.getDepartmentName()%>
                        </td>
                        <td>
                        <%=e.getJobName()%>
                        </td>
                        <td>
                        <div style="float: left;">
                        <a href="employeeupdate.jsp?id=<%=e.getEmployeeId()%>"><input type="button" class="btn btn-sm btn-warning" value="修改" ></a>
